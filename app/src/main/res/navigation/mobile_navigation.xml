<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.sheela.shopactivity.SearchFragment"
        android:label="Search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.sheela.shopactivity.CartFragment"
        android:label="Cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sheela.shopactivity.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_account"
        android:name="com.sheela.shopactivity.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />



    <fragment
        android:id="@+id/navigation_wishlist"
        android:name="com.sheela.shopactivity.WishlistFragment"
        android:label="Wishlist"
        tools:layout="@layout/fragment_wishlist" />
